import boto3
import json
AMI = 'ami-0fb653ca2d3203ac1'
INSTANCE_TYPE = 't2.micro'
KEY_NAME = 'sshkey1'
REGION = 'us-east-2' 

ec2 = boto3.client('ec2', region_name=REGION)

def lambda_handler(event, context):
     instance = ec2.run_instances(ImageId=AMI,InstanceType=INSTANCE_TYPE,KeyName=KEY_NAME,MaxCount=1,MinCount=1)
     print ("New instance created:")
     instance_id = instance['Instances'][0]['InstanceId']
     print (instance_id)
     return instance_id
